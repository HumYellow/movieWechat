<!--pages/myCenter/myCenterPage/myCenterPage.wxml-->
<view class="userBox">
    <view class="userHead">
      <image wx:if="{{userHeadPic}}" class="userHeadPic" mode="widthFix" src="{{userHeadPic}}"></image>
      <image wx:else class="userHeadPic" mode="widthFix" src="data:image/png;base64,"></image>
    
    </view>
    <view wx:if="{{userName}}" class="userDetail">
        <view class="userName">
            <view>{{userName}}</view>
            <view class="userType">
                <view class="userType-item">权益卡用户</view>

            </view>
        </view>
        <view class="getOurCode">
              获取无纸化验票号码 >
        </view>
    </view>
    <!-- <button wx:else class="loginBtn userName"
      open-type="getUserInfo"
      bindgetuserinfo="toWxLogin">
      点击登录
    </button> -->
    <view wx:else class="userDetail" bindtap="toLogin">登录</view>
    
  <!-- <view bindtap='toBindPhone'>绑定手机</view> -->
</view>
<view class="myCenterBox">
  <view class="myCenterMod" bindtap='toLink' data-url="/pages/order/orderList/orderList">
      <image class="myCenterMod-image" mode="widthFix" src="/static/image/myCenter/icon_cardbag.png"></image>
      <view class="myCenterMod-text">我的卡包</view>
  </view>
  <view class="myCenterMod" bindtap='toLink' data-url="/pages/myCenter/couponList/couponList">
      <image class="myCenterMod-image" mode="widthFix" src="/static/image/myCenter/icon_coupon.png"></image>
      <view class="myCenterMod-text">优惠券</view>
  </view>
  <view class="myCenterMod" bindtap='toTabBar' data-url="/pages/order/orderList/orderList">
      <image class="myCenterMod-image" mode="widthFix" src="/static/image/myCenter/icon_order.png"></image>
      <view class="myCenterMod-text">我的订单</view>
  </view>
  <view class="myCenterMod" bindtap='toLink' data-url="/pages/myCenter/wantWatch/wantWatch">
      <image class="myCenterMod-image" mode="widthFix" src="/static/image/myCenter/icon_like.png"></image>
      <view class="myCenterMod-text">想看的电影</view>
  </view>
  <!-- <view class="myCenterMod" bindtap='getScancode' data-url="/pages/order/orderList/orderList">
      <image class="myCenterMod-image" mode="widthFix" src="/static/image/myCenter/order.png"></image>
      <view class="myCenterMod-text">扫一扫</view>
  </view> -->
</view>
