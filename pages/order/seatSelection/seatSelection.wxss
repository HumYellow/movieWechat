/* pages/order/seatSelection/seatSelection.wxss */
.moveiBox{display:flex;padding:15px 40px;align-items: center;}
.moveiBox-pic{width:60px;}
.moveiBox-desc{padding-left:10px;}
.moveiBox-title{display: flex;align-items: flex-end;}
.moveiBox-movieName{margin-right:5px;font-size:16px;}
.moveiBox-movieType{font-size:12px;color:#666;}
.moveiBox-time{color:#666;font-size:12px;margin:6px 0;}
.moveiBox-cinemaType{display:flex;}
.moveiBox-cinemaType-item{font-size:10px;padding:2px 5px;border:1px solid #63a4fd;border-radius:3px;color:#63a4fd;margin-right:5px;}

.seatTipsList{display:flex;align-items: center;margin:0 auto;justify-content: center;padding:15px 0;font-size:14px;color:#333;border-top:1px solid #ebebeb}
.seatModTipsBox{display:flex;margin:0 10px;}
.seatModTips{width:20px;height:18px;margin:0 5px;}
.seatModTipsText{}

/* 选座 */
.seatPicMod{width:100%;box-sizing: border-box;position:relative;padding:0 20px 30px;background:#f7f7f7;border-bottom:1px solid #ebebeb;}
.seatPic{width:100%;overflow:auto;box-sizing: border-box;position:relative;padding:50px 0;margin:0 auto;}
.seatList{}
.seatBox{width:100%;}
.seatMod{width:20px;height:20px;box-sizing: border-box;float:left;margin:2px;}
.seatMod_N{background: url('data:image/png;base64,') no-repeat; background-size:100% auto;}
.seatMod_L{background: url('data:image/png;base64,') no-repeat; background-size:100% auto;}
.seatMod_S{background: url('data:image/png;base64,') no-repeat; background-size:100% auto;}
.seatModPic{width:100%;}
.midLine{position:absolute;width:1px;top:50px;border-left:1px dashed #b3b3b3}
.screenCenter{position:absolute;top:0;padding:2px 10px;text-align:center;font-size:14px;background:#e8e8e8;}
.seatNumList{position:absolute;top:45px;left:0;background:rgba(0,0,0,.4);padding:2px 2px 2px 0;border-radius:15px;color:#fff;width:20px;text-align:center;}
.seatNumList-item{height:24px;line-height:24px;font-size:12px;}

.seatCouponList{padding:10px;}
.seatCouponList-item{text-align:center;padding:5px 0;display:inline-block;margin:5px 1%;width:23%;
  background:url('data:image/png;base64,'); background-size:100% 100%;background-position:center center;}
.seatCouponList-item-box{display:flex;justify-content: center;align-items: center;}
.seatCouponList-item .seatNum{font-size:14px; color:#333;}
.seatCouponList-item .price{font-size:10px;color:#ff4d64;}
.seatCouponList-delect{width:8px;height:8px;margin-left:8px;}

.toBuyBox{position:fixed;left:0;bottom:0;width:100%;padding:5px 0;}
.toBuy{width:90%;height:40px;line-height:40px;text-align:center;background:#63a4fd;color:#fff;border-radius:20px;margin:0 auto;font-size:16px;}